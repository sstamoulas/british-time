{
  "hosting": {
    "public": "client/build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/image-upload",
        "function": "image-upload"
      },
      {
        "source": "/video-conference",
        "function": "video-conference"
      }
    ]
  }
}
